{
  "name": "Basketball Reference Extensions",
  "version": "0.3",
  "manifest_version": 3,
  "description": "Visualization for basketball-reference.com",
  "content_scripts": [
    {
      "matches": ["http://www.basketball-reference.com/*"],
      "js": ["d3.min.js", "index.js"],
      "css": ["index.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  }
}
